{
    "AWSEBDockerrunVersion": 2,
    "ContainerDefnitions":[
        {
            "name":"client",
            "Image":"sumanthgowda/multiclient",
            "hostname":"client",
            "essentil":false,
            "memory":128
        },
        {
            "name":"server",
            "Image":"sumanthgowda/multiserver",
            "hostname":"api",
            "essentil":false,
            "memory":128
        },
        {
            "name":"worker",
            "Image":"sumanthgowda/multiworker",
            "hostname":"worker",
            "essentil":false,
            "memory":128
        },
        {
            "name":"nginx",
            "Image":"sumanthgowda/multinginx",
            "hostname":"nginx",
            "essentil":true,
            "memory":128,
            "portMappings":[{
                "hostPort":80,
                "containerPort":80
            }],
            "links":["client","server"]
        }
    ]
}