{
    "AWSEBDockerrunVersion": "1",
    "containerDefinitions":[
        {
            "name":"client",
            "image":"sumanthgowda/multiclient",
            "hostname":"client",
            "essential":false,
            "memory":128
        },
        {
            "name":"server",
            "image":"sumanthgowda/multiserver",
            "hostname":"api",
            "essential":false,
            "memory":128
        },
        {
            "name":"worker",
            "image":"sumanthgowda/multiworker",
            "hostname":"worker",
            "essential":false,
            "memory":128
        },
        {
            "name":"nginx",
            "image":"sumanthgowda/multinginx",
            "hostname":"nginx",
            "essential":true,
            "memory":128,
            "portMappings":[{
                "hostPort":80,
                "containerPort":80
            }],
            "links":["client","server"]
        }
    ]
}